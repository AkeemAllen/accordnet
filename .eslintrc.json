{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "no-trailing-spaces": "off"
  }
}
